<Page xmlns="http://schemas.nativescript.org/tns.xsd" xmlns:ui="@evanyung/nativescript-amap" navigatingTo="navigatingTo" class="page">
    <Page.actionBar>
        <ActionBar title="nativescript-amap" icon="" class="action-bar">
        </ActionBar>
    </Page.actionBar>
    <StackLayout class="p-20">
        <GridLayout rows="auto,*">
          <Button row="0" text="Test nativescript-amap" tap="{{ testIt }}" class="btn btn-primary"/>
          <ui:AMapView row="1" aMapOptions="{{ aMapOptions }}" mapReady="{{ onMapReady }}" />
        </GridLayout>
    </StackLayout>
</Page>
